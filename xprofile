# Load Xresources
xrdb -load ~/.Xresources

#/etc/X11/xinit/xinitrc.d/50-systemd-user.sh &

# set mic vol
pactl set-source-volume @DEFAULT_SOURCE@ 70%

# Start compositor
pgrep picom && killall picom && sleep 2
picom --experimental-backends -b

xset r rate 250 28
#xinput set-prop 10 359 0, 1
xset s 600 120 -dpms
#xss-lock -- i3lock-fancy-rapid 5 3 -n &
#xset s off -dpms

# start some stuff
pgrep redshift && killall redshift && sleep 2
redshift &

#nm-applet -b &
telegram-desktop &

#discordrp-mpris &
